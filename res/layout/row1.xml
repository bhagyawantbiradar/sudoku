<?xml version="1.0" encoding="utf-8"?>
 <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
<TableLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="*"
        android:background="#00ff00">
    <TableRow android:layout_width="fill_parent">
        <TableLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="*"
                android:layout_margin="2dp">

            <TableRow android:layout_width="fill_parent">
                <EditText android:text="1"
                        style="@style/cell_style"
                        android:id="@+id/r1_c1"/>
                <EditText android:text="2"
                        style="@style/cell_style"
                        android:id="@+id/r1_c2"/>
                <EditText  android:text="3"
                        style="@style/cell_style"
                        android:id="@+id/r1_c3"/>
            </TableRow>


            <TableRow android:layout_width="fill_parent">
                <EditText  android:text="4"
                          style="@style/cell_style"
                       android:id="@+id/r2_c1" />
                <EditText  android:text="5"
                          style="@style/cell_style"
                          android:id="@+id/r2_c2"/>
                <EditText  android:text="6"
                          style="@style/cell_style"
                        android:id="@+id/r2_c3"/>
            </TableRow>


            <TableRow android:layout_width="fill_parent">
                <EditText android:text="7"
                          style="@style/cell_style"
                        android:id="@+id/r3_c1"/>
                <EditText android:text="8"
                          style="@style/cell_style"
                        android:id="@+id/r3_c2"/>
                <EditText android:text="9"
                          style="@style/cell_style"
                        android:id="@+id/r3_c3"/>
            </TableRow>

        </TableLayout>

        <TableLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="*"
                android:layout_margin="2dp">

            <TableRow android:layout_width="fill_parent">
                <EditText android:text="1"
                          style="@style/cell_style"
                          android:id="@+id/r1_c4"/>
                <EditText android:text="2"
                          style="@style/cell_style"
                          android:id="@+id/r1_c5"/>
                <EditText  android:text="3"
                           style="@style/cell_style"
                           android:id="@+id/r1_c6"/>
            </TableRow>


            <TableRow android:layout_width="fill_parent">
                <EditText  android:text="4"
                           style="@style/cell_style"
                           android:id="@+id/r2_c4" />
                <EditText  android:text="5"
                           style="@style/cell_style"
                           android:id="@+id/r2_c5"/>
                <EditText  android:text="6"
                           style="@style/cell_style"
                           android:id="@+id/r2_c6"/>
            </TableRow>


            <TableRow android:layout_width="fill_parent">
                <EditText android:text="7"
                          style="@style/cell_style"
                          android:id="@+id/r3_c4"/>
                <EditText android:text="8"
                          style="@style/cell_style"
                          android:id="@+id/r3_c5"/>
                <EditText android:text="9"
                          style="@style/cell_style"
                          android:id="@+id/r3_c6"/>
            </TableRow>

        </TableLayout>
        <TableLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="*"
                android:layout_margin="2dp">

            <TableRow android:layout_width="fill_parent">
                <EditText android:text="1"
                          style="@style/cell_style"
                          android:id="@+id/r1_c7"/>
                <EditText android:text="2"
                          style="@style/cell_style"
                          android:id="@+id/r1_c8"/>
                <EditText  android:text="3"
                           style="@style/cell_style"
                           android:id="@+id/r1_c9"/>
            </TableRow>


            <TableRow android:layout_width="fill_parent">
                <EditText  android:text="4"
                           style="@style/cell_style"
                           android:id="@+id/r2_c7" />
                <EditText  android:text="5"
                           style="@style/cell_style"
                           android:id="@+id/r2_c8"/>
                <EditText  android:text="6"
                           style="@style/cell_style"
                           android:id="@+id/r2_c9"/>
            </TableRow>


            <TableRow android:layout_width="fill_parent">
                <EditText android:text="7"
                          style="@style/cell_style"
                          android:id="@+id/r3_c7"/>
                <EditText android:text="8"
                          style="@style/cell_style"
                          android:id="@+id/r3_c8"/>
                <EditText android:text="9"
                          style="@style/cell_style"
                          android:id="@+id/r3_c9"/>
            </TableRow>

        </TableLayout>
    </TableRow>
    <include layout="@layout/row2"/>
    <include layout="@layout/row3"/>

</TableLayout>

</LinearLayout>
